{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualMachines_basicvm1_name": {
            "defaultValue": "basicvm1",
            "type": "String"
        },
        "disks_basicvm1_OsDisk_1_0b1c5139f35c4b93b8cd0503d58fe528_externalid": {
            "defaultValue": "/subscriptions/39871908-78ed-4ef9-adbf-cb5c701334e3/resourceGroups/ARMTESTING1/providers/Microsoft.Compute/disks/basicvm1_OsDisk_1_0b1c5139f35c4b93b8cd0503d58fe528",
            "type": "String"
        },
        "networkInterfaces_basicvm1197_externalid": {
            "defaultValue": "/subscriptions/39871908-78ed-4ef9-adbf-cb5c701334e3/resourceGroups/armtesting1/providers/Microsoft.Network/networkInterfaces/basicvm1197",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Compute/virtualMachines",
            "apiVersion": "2019-07-01",
            "name": "[parameters('virtualMachines_basicvm1_name')]",
            "location": "eastus",
            "tags": {
                "acctTag": "001"
            },
            "properties": {
                "hardwareProfile": {
                    "vmSize": "Standard_DS1_v2"
                },
                "storageProfile": {
                    "imageReference": {
                        "publisher": "MicrosoftWindowsServer",
                        "offer": "WindowsServer",
                        "sku": "2016-Datacenter",
                        "version": "latest"
                    },
                    "osDisk": {
                        "osType": "Windows",
                        "name": "[concat(parameters('virtualMachines_basicvm1_name'), '_OsDisk_1_0b1c5139f35c4b93b8cd0503d58fe528')]",
                        "createOption": "FromImage",
                        "caching": "ReadWrite",
                        "managedDisk": {
                            "storageAccountType": "Standard_LRS",
                            "id": "[parameters('disks_basicvm1_OsDisk_1_0b1c5139f35c4b93b8cd0503d58fe528_externalid')]"
                        },
                        "diskSizeGB": 127
                    },
                    "dataDisks": []
                },
                "osProfile": {
                    "computerName": "[parameters('virtualMachines_basicvm1_name')]",
                    "adminUsername": "ad",
                    "windowsConfiguration": {
                        "provisionVMAgent": true,
                        "enableAutomaticUpdates": true
                    },
                    "secrets": [],
                    "allowExtensionOperations": true,
                    "requireGuestProvisionSignal": true
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[parameters('networkInterfaces_basicvm1197_externalid')]"
                        }
                    ]
                },
                "diagnosticsProfile": {
                    "bootDiagnostics": {
                        "enabled": true,
                        "storageUri": "https://armtesting1diag.blob.core.windows.net/"
                    }
                },
                "priority": "Spot",
                "evictionPolicy": "Deallocate",
                "billingProfile": {
                    "maxPrice": -1
                }
            }
        }
    ]
}